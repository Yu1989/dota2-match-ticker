{
  "name": "dota2-match-ticker",
  "version": "1.0.0",
  "description": "An single page site with Dota2 match schedules, and nothing else.",
  "main": "dist/app.js",
  "scripts": {
    "build": "rm -rf dist; babel src -d dist; webpack --progress --colors",
    "test": "standard & mocha",
    "dev": "pm2 start src/app.js --watch --interpreter babel-node; pm2 logs --raw | bunyan -o short; webpack --watch",
    "kill": "pm2 kill",
    "deploy": "pm2 deploy production",
    "logs": "pm2 logs --raw | bunyan -o short"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yu1989/dota2-match-ticker.git"
  },
  "keywords": [
    "dota",
    "dota2",
    "match",
    "ticker",
    "schedule"
  ],
  "author": "Yu Jiang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Yu1989/dota2-match-ticker/issues"
  },
  "homepage": "https://github.com/Yu1989/dota2-match-ticker#readme",
  "dependencies": {
    "bluebird": "^3.4.0",
    "bunyan": "^1.8.1",
    "cheerio": "^0.20.0",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-views": "^5.0.1",
    "lodash": "^4.13.1",
    "moment": "^2.13.0",
    "pug": "^2.0.0-alpha8",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "request-promise": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "css-loader": "^0.23.1",
    "gulp": "^3.9.1",
    "mocha": "^2.5.3",
    "node-sass": "^3.7.0",
    "pm2": "^1.1.3",
    "sass-loader": "^3.2.0",
    "standard": "^7.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
